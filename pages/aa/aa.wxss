.box {
  position: fixed;
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  background: #fff;
}

.box1 {
  width: 100%;
  flex: 1;
}

.box11 {
  border-bottom: 20rpx solid #eee;
}

.box12 {
}

.box13 {
  border-bottom: 20rpx solid #eee;
}

.box-img2 {
  width: 146rpx;
  height: 102rpx;
  display: block;
  margin: 90rpx auto 0;
}

.box-img1 {
  width: 146rpx;
  height: 145rpx;
  display: block;
  margin: 90rpx auto 0;
}

.box-text {
  font-size: 36rpx;
  text-align: center;
  margin-top: 20rpx;
}

.box-text2 {
  font-size: 36rpx;
  text-align: center;
  margin-top: 50rpx;
}

.box-room {
  width: 400rpx;
  height: 100rpx;
  margin: 50rpx auto;
  position: relative;
}

.box-rooms {
  box-sizing: border-box;
  line-height: 100rpx;
  background-color: #fff;
  text-align: center;
  position: absolute;
  font-size: 55rpx;
  font-weight: 800;
  top: 0;
  height: 100rpx;
  width: 100rpx;
  border: 1px solid #eee;
}

.room-code-input {
  width: 400rpx;
  height: 100rpx;
  position: absolute;
  left: -1000rpx;
  top: 0;
  color: #fff;
  z-index: -999;
  opacity: 0;
}

.room-code-input:active {
  z-index: -99;
}

.box-room view:nth-child(2) {
  left: 0rpx;
}

.box-room view:nth-child(3) {
  left: 100rpx;
}

.box-room view:nth-child(4) {
  left: 200rpx;
}

.box-room view:nth-child(5) {
  left: 300rpx;
}

.border0 {
  border-left: none;
}
